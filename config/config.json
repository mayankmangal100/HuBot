{
    "model": {
        "embedding_model": "BAAI/bge-base-en-v1.5",
        "llm_model_path": "Qwen2.5-1.5B-Instruct.Q5_K_M_MP.gguf",
        "device": "cpu"
    },
    "vector_store": {
        "engine": "qdrant",
        "collection_name": "my_collection",
        "location": ":memory:",
        "use_faiss_fallback": false
    },
    "retrieval": {
        "top_k": 3,
        "alpha": 0.5
    },
    "chunking": {
        "chunk_size": 512,
        "chunk_overlap": 50 
    },
    "llm": {
        "max_tokens": 256,
        "temperature": 0.1,
        "top_p": 0.8,
        "repeat_penalty": 1.5,
        "top_k": 20,
        "context_length": 4096,
        "batch_size": 512,
        "threads": 4,
        "min_p": 0.05
    }
}
