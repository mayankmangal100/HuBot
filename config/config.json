{
    "model": {
        "embedding_model": "BAAI/bge-base-en-v1.5",
        "llm_model_path": "Qwen2.5-1.5B-Instruct.Q5_K_M_MP.gguf",
        "device": "cpu"
    },
    "vector_store": {
        "engine": "qdrant",
        "collection_name": "my_collection",
        "location": ":memory:",
        "use_faiss_fallback": false
    },
    "retrieval": {
        "top_k": 3,
        "alpha": 0.5
    },
    "chunking": {
        "chunk_size": 512,
        "chunk_overlap": 50 
    },
    "llm": {
        "max_tokens": 200,
        "temperature": 0.3,
        "top_p": 0.9,
        "repeat_penalty": 1.1,
        "top_k": 40
    }
}
